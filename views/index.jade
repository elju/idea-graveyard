!!! 5
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Fredericka+the+Great')
      link(rel='stylesheet', href='stylesheets/github.css')
      link(rel='stylesheet', href='stylesheets/main.css')
      link(rel='stylesheet', href='css/bootstrap.min.css')
      link(rel='stylesheet', href='css/font-awesome.min.css')
      link(rel='stylesheet', href='css/font-awesome-ie7.min.css')
      link(rel='stylesheet', href='stylesheets/extra.css')
      script(src='javascripts/jquery-2.0.0.min.js')
      script(src='javascripts/main.js')
  body
    include navbar
    div.cont
      div#row-fluid
        a.slice#one(href="#onebox")
          div.subwindow
            .container-fluid
              - var arr = (function(){for(var i=0, temp=[];i<40;i++){temp.push(Math.floor(Math.random()*22)+1);}
              -     return temp;})();
              - console.log(arr);
              each number, i in arr
                .artwrapper
                  i.icon-arrow-up.icon-2x.arrow
                  img(src='images/art/mediums/artwork_#{number}_medium.jpg')
                  i.icon-arrow-down.icon-2x.arrow
        a.slice#two(href="#twobox")
          div.subwindow
            .container-fluid
        a.slice#three(href="#threebox")
          div.subwindow
            .container-fluid
              .table
                .subones
                  h3 Learn more!
                .subones
                  h3 Even More!
              include other
        a.slice#four(href="#fourbox")
          div.subwindow
            .container-fluid
    div.mainbar
